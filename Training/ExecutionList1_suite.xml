<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" >
<listeners>
	<listener class-name="listeners.Listeners"/>
</listeners>
    <test thread-count="1" parallel = "classes" name="Create ACH Outward">
	<groups>
            <run>
                <include name="CreateACH"/>
                <include name="VerifyACH"/>
                <include name="AuthorizeACH"/>
            </run>
        </groups>

         <classes>
			<class name="tests.ABE_Create_ACH_Outward_Test"/>	
			<class name="tests.ABE_Verify_ACH_Outward_Test"/>
			<class name="tests.ABE_Authorize_ACH_Outward_Test"/>
		</classes>
     </test>
</suite>