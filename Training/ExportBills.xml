        </run>
    </groups>
    <classes>
		<class name="tests.ABELodgeExportBillsTradeFinanceTest"/>
		<class name="tests.ABEVerifyLodgeExportBillsTradeFinanceTest"/>
		<class name="tests.ABEAcceptExportBillsTradeFinanceTest"/>
		<class name="tests.ABEVerifyAcceptExportBillsTradeFinanceTest"/>
		<class name="tests.ABEPurchaseExportBillsTradeFinanceTest"/>
		<class name="tests.ABEVerifyPurchaseExportBillsTradeFinanceTest"/>
