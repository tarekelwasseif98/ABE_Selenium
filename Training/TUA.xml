<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
	<listener class-name="listeners.Listeners"/>
    <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
</listeners>
<test thread-count="1" name="TUA">
	<groups>
		<run>
			<include name="ABEOpenTUATest"/>
            <include name="ABEVerifyTUAOpeningTest"/>
            <include name="ABECloseTUATest"/>
            <include name="ABEVerifyTUAClosureTest"/>
        </run>
    </groups>
    <classes>
		<class name="tests.ABEOpenTUATest"/>
	 	<class name="tests.ABEVerifyTUAOpeningTest"/>
	 	<class name="tests.ABECloseTUATest"/>
	 	<class name="tests.ABEVerifyTUAClosureTest"/>
    </classes>
</test>
</suite>